@base <http://arga.org.au/schemas/fields/>.

@prefix :        <http://arga.org.au/schemas/mapping/>.
@prefix source:  <http://arga.org.au/source/>.
@prefix formats: <http://arga.org.au/source/formats/>.
@prefix ncbi:    <http://arga.org.au/schemas/maps/ncbi/>.

source:assembly_summary_genbank.txt :format          formats:tsv;
                                    :transforms_into ncbi:deposition.


ncbi:deposition
{
  <entity_id>   :hash ncbi:\#assembly_accession.
  <assembly_id> :same ncbi:\#assembly_accession.

  <event_date>  :same ncbi:seq_rel_date.
  <url>         :same ncbi:ftp_path.
  <institution> :same ncbi:asm_submitter.
}
