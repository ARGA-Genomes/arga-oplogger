@base <http://arga.org.au/schemas/fields/>.

@prefix :       <http://arga.org.au/schemas/mapping/>.
@prefix model:  <http://arga.org.au/model/>.
@prefix ncbi:   <http://arga.org.au/schemas/maps/ncbi/>.

model:assembly_summary_genbank :models ncbi:annotation.
model:assembly_summary_genbank.txt :models ncbi:annotation.


ncbi:annotation
{
  <entity_id>                :hash ncbi:\#assembly_accession.
  <assembly_id>              :same ncbi:\#assembly_accession.

  <name>                     :same ncbi:annotation_name.
                             # :ignores "na".

  <provider>                 :same ncbi:annotation_provider.
                             # :ignores "na".

  <event_date>               :same ncbi:annotation_date.

  <number_of_genes>          :same    ncbi:total_gene_count.
                             # :ignores "na".

  <number_of_proteins>       :same    ncbi:protein_coding_gene_count.
                             # :ignores "na".
}
